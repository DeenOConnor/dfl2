	"libs": [
				"../debug/dfl_debug",
				"ole32",
				"oleAut32",
				"gdi32",
				"comctl32",
				"comdlg32",
				"advapi32",
				"uuid",
				"ws2_32"
			],
	"lflags": ["/su:console:4"]
}