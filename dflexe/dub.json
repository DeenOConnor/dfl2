{

	"name": "dflexe",
	"description": "dflexe is a build tool for dfl2, it can be built by DUB and Visual Studio .NET(such as VS 2010).",
	"homepage": "https://github.com/FrankLIKE/dfl2",
	"targetName": "dflexe",
	"mainSourceFile": "source/dflexe.d",
	"targetPath": "../debug",
