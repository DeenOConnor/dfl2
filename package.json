{
	"name": "dfl2",
	"description" : "dfl2 IS A GUI  only for Windows,now it can be built by Dub and Visual Studio .NET, such as VS 2010.",
	"homepage": "https://github.com/FrankLike/dfl2",
	"license" : "LGPL-3.0",
	"authors" : ["Christopher E. Miller"],
	"dependencies" : {
	},
 
	"configurations" : [
	 	{
			"name": "dfl_debug",
			"targetType": "library",
			 "targetPath" : "debug",
			"targetName": "dfl_debug",
            "libs":[
            		"ole32",
            		"oleAut32",
            		"gdi32",
            		"comctl32",
            		"comdlg32",
            		"advapi32",
            		"uuid",
            		"ws2_32"
            		]
		},
			 
		{
			"name": "dfl",
			"targetType": "library",
			 "targetPath" : "release",
			"targetName": "dfl",
            "libs":[
            		"ole32",
            		"oleAut32",
            		"gdi32",
            		"comctl32",
            		"comdlg32",
            		"advapi32",
            		"uuid",
            		"ws2_32"
            		]
		},
		{
			"name": "dflexe",
			"targetType": "executable",
			 "targetPath" : "release",
			"mainSourceFile": "source/dfl_exe/dfl_exe.d",
			"lflags": ["/subsystem:console"],
			"targetName": "dfl_Release",
			 "libs":[
					"dfl",
			 		"ole32",
            		"oleAut32",
            		"gdi32",
            		"comctl32",
            		"comdlg32",
            		"advapi32",
            		"uuid",
            		"ws2_32"
			 ]
		},

	]
}

