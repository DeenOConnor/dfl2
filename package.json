{
	"name": "dfl2",
	"version": "1.0.1",
	"description": "dfl2 is a GUI library for Windows,now it can be built by DUB and Visual Studio .NET(such as VS 2010).",
	"license": "LGPL-3.0",
	"authors": [
		"Christopher E. Miller","FrankLIKE"
	],
	"homepage": "https://github.com/FrankLIKE/dfl2",
	"importPaths": [
		"source/"
	],
	"configurations": [
		{
			"name": "dfl_debug",
			"targetName": "dfl_debug",
			"targetType": "library",
			"targetPath": "debug",
			"source":["source/dfl","source/dfl/internal"],
			"libs": [
				"ole32",
				"oleAut32",
				"gdi32",
				"comctl32",
				"comdlg32",
				"advapi32",
				"uuid",
				"ws2_32"
			]
		},
		{
			"name": "dfl",
			"targetType": "library",
			"targetName": "dfl",
			"targetPath": "release",
			"source":["source/dfl","source/dfl/internal"],
			"libs": [
				"ole32",
				"oleAut32",
				"gdi32",
				"comctl32",
				"comdlg32",
				"advapi32",
				"uuid",
				"ws2_32"
			],
		},
		{
			"name": "dflexe",
			"targetName": "dflexe",
			"workingDirectory": "release",
			"mainSourceFile": "othersource/dflexe/source/dflexe.d",
			"buildTypes":"release",
			"targetPath": "release",
			"libs": ["dfl"],
			"targetType": "executable",
			"lflags": [
				"/subsystem:console"
			],

		}
	],

	
}